<%= form_for :car do |f| %>
  <%= f.file_field :avatar %>
	<br>
	<%=f.text_area :mark%>
	<br>
	<%@years=Array.new%>
	<%for i in 0..1500%>
		<%@years<<i%>
	<%end%> 
	<%=f.label :horsepower%>
	<%=f.label :wheeldrive%>
	<br>
	<%= f.select :horsepower, ((@years))%>
	<%= f.select :wheeldrive, ['front', 'rear','all']%>
	<br>
	<br>
	<%=f.submit%>
<%end%>